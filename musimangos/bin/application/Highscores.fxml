<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="HighscoresPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.MusimangosController">
   <children>
      <Button fx:id="menubutton" layoutX="279.0" layoutY="364.0" mnemonicParsing="false" onAction="#showMenuScene" text="Menu" />
      <Label contentDisplay="CENTER" layoutX="25.0" prefHeight="91.0" prefWidth="200.0" text="Hiscores! \^-^/" />
      <TextArea fx:id="HighscoresTextArea" editable="false" layoutX="25.0" layoutY="64.0" prefHeight="270.0" prefWidth="200.0" />
      <TextField fx:id="ScoreField" layoutX="271.0" layoutY="67.0" />
      <Text layoutX="271.0" layoutY="64.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Name" />
      <Button fx:id="AddScoreButton" layoutX="338.0" layoutY="94.0" mnemonicParsing="false" onAction="#updateHiscores" text="Add score" />
      <Button fx:id="LoadScoresButton" layoutX="151.0" layoutY="334.0" mnemonicParsing="false" onAction="#displayHighscores" text="Load scores" />
   </children>
</AnchorPane>
